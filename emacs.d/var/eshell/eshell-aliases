alias mpv-screenshots mpv --screenshot-directory="./" --screenshot-jpeg-quality=100 $*
alias ym youtube-dl -x --audio-format mp3  "$*"
alias y youtube-dl -i -c --all-sub -f bestvideo[height<=?720]+bestaudio/best "$*"
alias o xdg-open $*
alias ll ls -lh $*
alias dut du -h $* | tail -1
alias df df -h
alias apts apt-cache search $*
alias apti sudo apt-get install $*
alias aptS apt-cache show $*
alias aptp apt-cache policy $*
alias l ls -lha
